{
  "name": "battle-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down --volumes",
    "db:seed": "dotenv -e .env ts-node prisma/seed.ts --experimental-specifier-resolution=node",
    "prisma:justdoit": "dotenv -e .env yarn prisma:migrate && yarn prisma:generate",
    "prisma:migrate": "yarn prisma migrate save --experimental && yarn prisma migrate up --experimental",
    "prisma:generate": "yarn prisma generate",
    "prisma:introspect": "yarn prisma introspect",
    "prisma:studio": "yarn prisma studio --experimental"
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@prisma/client": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.4.5-canary.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.20",
    "@types/react": "^16.9.41",
    "dotenv-cli": "^3.2.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.6"
  }
}
