{
  "name": "battle",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down --volumes",
    "db:seed": "dotenv -e .env ts-node --project prisma/tsconfig.json prisma/seed.ts --type-check",
    "db:drop": "dotenv -e .env ts-node --project prisma/tsconfig.json prisma/clearDB.ts --type-check",
    "prisma:justdoit": "dotenv -e .env yarn prisma:migrate && yarn prisma:generate",
    "prisma:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "prisma:generate": "prisma generate",
    "prisma:introspect": "prisma introspect",
    "prisma:s": "prisma studio --experimental",
    "prisma:studio": "dotenv -e .env yarn prisma:s"
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@prisma/client": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^11.1.3",
    "pluralize": "^8.0.0",
    "react": "^16.13.1",
    "react-content-loader": "^5.1.0",
    "react-dom": "^16.13.1",
    "swr": "0.3.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.5.0",
    "@tailwindcss/typography": "^0.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.6.0",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^16.9.46",
    "dotenv-cli": "^3.2.0",
    "faker": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.7.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
