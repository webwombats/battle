datasource db {
  provider = "postgresql"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model World {
  id         Int    @id @default(autoincrement())
  name       String @unique
  population Float
}

model Battle {
  id          Int   @id @default(autoincrement())
  description Float
}
